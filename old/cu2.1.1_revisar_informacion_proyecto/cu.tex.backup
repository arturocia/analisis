	\begin{UCtrayectoria}{Principal}
			\UCpaso[\UCactor] Elije el Proyecto del menu \textit{MENU DE DONDE ELIJE ESTA COSA!!!}.
			\UCpaso Obtiene los datos del Proyecto seleccionado.
			\UCpaso Obtiene las rutas de las alineaciones con los elementos EP1N. 
			\UCpaso Muestra la pantalla \IUref{IURevisarInformacionProyecto}{Revisar Informacion de Proyecto} con la informacion obtenida. \label{paso:CU2.1.1MuestraInformacion}.
	\end{UCtrayectoria}

