	\begin{UseCase}{CU2.1.1}{Revisar Informacion de Proyecto}{Revisar la informacion de un Proyecto asociado a un Coordinador.}
		\UCitem{Versión}{1.0}
		\UCitem{Actor(es)}{Coordinador, Gerente}
		\UCitem{Propósito}{Revisar la informacion de un Proyecto registrado.}
		\UCitem{Resumen}{Se muestra toda la informacion del proyecto, datos de responsable, datos de proyecto, asi como la informacion de su alineacion.}
		\UCitem{Entradas}{Ninguno.}
		\UCitem{Salidas}{Informacion de Proyecto \ref{dd:RevisarInformacionProyecto} .}
